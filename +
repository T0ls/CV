		<script>
		// Box in alto fix altezza
		var box1 = document.getElementById("nameCard")
		document.getElementById("contactCard").style.height = box1.offsetHeight + "px";
		
		function arrowDown(str) {
			var arrow = document.getElementById('arrow' + str);
			arrow.innerHTML = "⮛";
		}
		
		// Show Arrow Menù
		function arrowUp(str) {
			var arrow = document.getElementById('arrow' + str);
			arrow.innerHTML = "⮚";
		}
		function arrowController(str) {
		//document.getElementById('button' + str).addEventListener('click', function() {
			if (document.getElementById('arrow' + str).innerHTML === "⮚") {
				arrowDown(str);
			} else {
				arrowUp(str);
			}
		}

		var tablePx = document.getElementsByTagName("tr")
		for(var i=0; i<tablePx.length; i++) {
			var tableChild = tablePx[i].firstElementChild;
			tableChild.style.width = "25%";
		}

		// Update current date
		var annoCurrP = document.getElementById("annoCorrente");
		var oggi = new Date();
		var annoCorrente = oggi.getFullYear();
		annoCurrP.innerHTML = annoCorrente;

		</script>
